@model IEnumerable<TreeOfLife.Model.Element>

@{
    ViewBag.Title = "ElementList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ElementList</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KeyScale)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Symbol)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KeyScale)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Symbol)
        </td>
    </tr>
}

</table>
