@model IEnumerable<TreeOfLife.Model.Planet>

@{
    ViewBag.Title = "PlanetList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>PlanetList</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KeyScale)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Glyph)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KeyScale)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Glyph)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>